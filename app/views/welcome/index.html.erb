<h1>Welcome to Viewing Party!</h1>

<% if session[:user_id] %>
  <%= link_to "Log Out", "/logout", method: :delete %>
<% else  %>
  <%= button_to "Create a New User", "/register", method: :get %>
  <%= link_to "Log In", '/login' %>
<% end  %>

<% @users.each do |user| %>
  <p><%=link_to "#{user.name}", "/dashboard" %></p>
<% end %>
